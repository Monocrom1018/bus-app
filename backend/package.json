{
  "name": "bone-node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:development": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "db:seed": "nest build && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "db:migrate": "ts-node -P tsconfig.build.json ./node_modules/typeorm/cli.js migration:run",
    "db:rollback": "ts-node -P tsconfig.build.json ./node_modules/typeorm/cli.js migration:revert",
    "db:migrations": "yarn db:migrate && nest build && ts-node -P tsconfig.build.json ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@admin-bro/design-system": "^1.7.3",
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/nestjs": "^1.1.0",
    "@admin-bro/typeorm": "^1.4.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/express": "^4.17.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.6",
    "@types/passport-local": "^1.0.33",
    "admin-bro": "^3.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "fs": "^0.0.1-security",
    "geolib": "^3.3.1",
    "helmet": "^4.4.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nestjs-admin": "^0.4.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pluralize": "^8.0.0",
    "qs": "^6.10.1",
    "randombytes": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.31",
    "typeorm-cli": "^1.0.7",
    "typeorm-polymorphic": "^0.0.10",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.15",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "run-script-webpack-plugin": "^0.0.11",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^5.38.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
