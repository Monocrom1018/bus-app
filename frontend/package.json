    "dev": "cross-env NODE_ENV=development webpack serve --config ./build/webpack.config.js --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production node ./build/build.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production node ./build/build.js && npx cap copy android",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/",
    "prettier": "prettier --write --config ./prettier.config.js './src/**/*.{ts,tsx}'",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "deploy": "cross-env NODE_ENV=production node ./build/build.js && cd react-cdk && yarn cdk deploy",
    "cognito-mock": "./node_modules/.bin/cognito-mock",
    "cognito-create-pool": "./node_modules/.bin/cognito-mock create-pool --user-pool-id=local_bus"
  },
